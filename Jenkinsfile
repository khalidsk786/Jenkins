pipeline {
  agent any 
    stages {
       
       stage ('build')
       {
         steps {

            echo "this is building"
         }
 

       }

       
       stage ('test')
       {
            
         steps {

            echo "this is testing"
         }
        
       }

       
       stage ('deploy')
       { 
           echo "this is deploy"
        
       }






    }



}